<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/naturalezac"
    android:backgroundTint="#6BF4F2F2"
    android:backgroundTintMode="src_atop"
    tools:context=".ui.dashboard.DashboardFragment">

    <Button
        android:id="@+id/btnescaner"
        android:layout_width="141dp"
        android:layout_height="90dp"
        android:layout_gravity="start"

        android:layout_marginTop="70dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/camara"
        android:textColor="#F6F3F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnalta"
        android:layout_width="141dp"
        android:layout_height="90dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"
        android:layout_marginTop="70dp"

        android:layout_marginEnd="48dp"
        android:background="@drawable/siloi"
        android:textColor="#F6F3F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnenviar"
        android:layout_width="141dp"
        android:layout_height="90dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"

        android:layout_marginTop="30dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/copia"
        android:textColor="#F6F3F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard" />

    <Button
        android:id="@+id/btnsoporte"
        android:layout_width="141dp"
        android:layout_height="90dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"

        android:layout_marginTop="30dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/soporte"
        android:textColor="#F6F3F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard4" />

    <Button
        android:id="@+id/btnreportes"
        android:layout_width="141dp"
        android:layout_height="90dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"

        android:layout_marginTop="30dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/reporte"
        android:textColor="#F6F3F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard2" />

    <Button
        android:id="@+id/btnusuario"
        android:layout_width="141dp"
        android:layout_height="90dp"
        android:layout_gravity="start"
        android:layout_marginStart="48dp"

        android:layout_marginTop="30dp"
        android:layout_marginEnd="48dp"
        android:background="@drawable/cliente"
        android:textColor="#F6F3F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_dashboard3" />

    <TextView
        android:id="@+id/text_dashboard2"
        android:layout_width="200dp"
        android:layout_height="27dp"
        android:layout_marginTop="28dp"
        android:text="SILOS"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.075"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnalta" />

    <TextView
        android:id="@+id/text_dashboard3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="200dp"
        android:text="REPORTES"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnreportes" />

    <TextView
        android:id="@+id/text_dashboard5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="200dp"
        android:text="USUARIOS"
        android:textAlignment="center"
        android:textColor="#0B0B0B"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnusuario" />

    <TextView
        android:id="@+id/text_dashboard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="8dp"
        android:text="ESCANEAR SILOS"
        android:textAlignment="center"
        android:textColor="#040404"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnescaner" />

    <TextView
        android:id="@+id/text_dashboard4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="8dp"
        android:text="BASE DE DATOS"
        android:textAlignment="center"
        android:textColor="#040404"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnenviar" />

    <TextView
        android:id="@+id/text_dashboard6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="8dp"
        android:text="SOPORTE"
        android:textAlignment="center"
        android:textColor="#0E0E0E"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnsoporte" />

</androidx.constraintlayout.widget.ConstraintLayout>